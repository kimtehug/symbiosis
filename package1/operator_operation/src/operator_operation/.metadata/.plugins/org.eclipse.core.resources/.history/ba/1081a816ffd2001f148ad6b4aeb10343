package package1;

public class Test4 {

	 int a ;
	int b;
	 void method() {
		 System.out.println("Calling from NonStsic method by Creating Object");
		System.out.println(a);
		System.out.println(b);	
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Test4 t = new Test4();
	
		t.a=10;
		t.b =12;
		t.method();
		
	
	}

}
