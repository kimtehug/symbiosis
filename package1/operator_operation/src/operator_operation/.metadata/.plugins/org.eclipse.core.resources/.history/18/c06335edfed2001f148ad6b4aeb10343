package package1;

public class Test2 {

	static int a ;
	static int b;
	static void method() {
		System.out.println("static method calling ");
		System.out.println("A:" +a);
		System.out.println("B:" +b);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		a=1500;
		b=100;
		method();
		
	
	}

}
